<html>
    <head>
        <link rel="stylesheet" href="/css/add_student.css">
    </head>
    <body>
        <form action="/student/create" method="post">
            <div>Name: <input type="text" name="name" id="name" required></div>
            <div>Email: <input type="text" name="email" id="email" required></div>
            <div>College: <input type="text" name="college" id="college" required></div>
            <div>DSA Score: <input type="number" name="dsa_score" id=""></div>
            <div>Web Dev Score: <input type="number" name="Webd_score" id=""></div>
            <div>React Score: <input type="number" name="react_score" id=""></div>
            <div>Placement Status: <input type="text" name="placement_status" id="" required></div>
            
            <input type="submit" value="Add Student">
        </form>
    </body>
    <script src="/js/add_student.js"></script>
</html>


